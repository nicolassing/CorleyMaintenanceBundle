{
    "name": "nicolassing/maintenance-bundle",
    "type": "symfony-bundle",
    "description": "Corley Maintenance Bundle",
    "keywords": ["Maintenance management", "503", "apache2", "apache", "nginx", "maintenance"],
    "license": "MIT",
    "replace": {
        "corley/maintenance-bundle": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Corley\\MaintenanceBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corley\\MaintenanceBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "^3.4|^4.2|^5.0",
        "symfony/security-bundle": "^3.4|^4.2|^5.0",
        "symfony/console": "^3.4|^4.2|^5.0",
        "symfony/dependency-injection": "^3.4|^4.2|^5.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "1.*",
        "phpunit/phpunit": "^8.0"
    }
}
